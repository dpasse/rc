{
  "compilerOptions": {
    "jsx": "react",
    "target": "es5",
    "module": "es6",
    "outDir": "./dist/",
    "moduleResolution": "Node",

    "strict": false,
    "allowJs": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "removeComments": true,
    "strictNullChecks": true,
    "noUncheckedIndexedAccess": true,
    "allowSyntheticDefaultImports": true,

    "rootDirs": [
      "src"
    ],

    "typeRoots": [],
  },
  "exclude": [
    "rc",
    "app",
    "node_modules",
  ],
}